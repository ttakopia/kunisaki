<p id="notice"><%= notice %></p>

<h1>Listing Events</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Title</th>
      <th>Opendate</th>
      <th> opendate memo</th>
      <th>Postal</th>
      <th>Address</th>
      <th>Address embed</th>
      <th>Fee</th>
      <th>Limit</th>
      <th>Integer</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @events.each do |event| %>
      <tr>
        <td><%= event.user_id %></td>
        <td><%= event.title %></td>
        <td><%= event.opendate %></td>
        <td><%= event. opendate_memo %></td>
        <td><%= event.postal %></td>
        <td><%= event.address %></td>
        <td><%= event.address_embed %></td>
        <td><%= event.fee %></td>
        <td><%= event.limit %></td>
        <td><%= event.integer %></td>
        <td><%= link_to 'Show', event %></td>
        <td><%= link_to 'Edit', edit_event_path(event) %></td>
        <td><%= link_to 'Destroy', event, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Event', new_event_path %>
